{
    "files": [
        "composer.json"
    ],
    "finder": [
        {
            "notName": "/README|.*\\.md|LICENSE|.*\\.md|phpunit5|.*\\.xml|.*\\.neon|.*\\.dist|Makefile|composer\\.json|composer\\.lock/",
            "exclude": [
                "bin",
                "docs",
                "bamarni",
                "behat",
                "doctrine",
                "myclabs",
                "hamcrest",
                "fzaninotto",
                "phpunit",
                "phpspec",
                "php_codesniffer",
                "phpdocumentor",
                "phar-io",
                "prophecy",
                "sebastian",
                "mockery",
                "test",
                "tests",
                "Test",
                "Tests",
                "Tester",
                "theseer",
                "webmozart"
            ],
            "in": "vendor"
        },
        {
            "exclude": [
                "tests"
            ],
            "in": "config"
        },
        {
            "exclude": [
                "logs"
            ],
            "in": "storage"
        },
        {
            "in": "app"
        },
        {
            "in": "bootstrap"
        }
    ],
    "php-scoper": null,
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ]
}
