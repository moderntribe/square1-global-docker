{
    "directories": [
        "app",
        "bootstrap",
        "config",
        "storage",
        "vendor"
    ],
    "files": [
        "composer.json"
    ],
    "finder": [
        {
            "exclude": [
                "docs",
                "bamarni",
                "behat",
                "doctrine",
                "myclabs",
                "hamcrest",
                "fzaninotto",
                "phpunit",
                "phpspec",
                "php_codesniffer",
                "phpdocumentor",
                "phar-io",
                "prophecy",
                "sebastian",
                "mockery",
                "test",
                "tests",
                "Test",
                "Tests",
                "Tester",
                "theseer",
                "webmozart",
                "vendor/nunomaduro/collision/src/Adapters/Laravel/Commands/TestCommand.php"
            ],
            "in": "vendor"
        }
    ],
    "php-scoper": null,
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ]
}
